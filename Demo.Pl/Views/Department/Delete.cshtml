@model Department
@{
    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>

<form asp-action="Delete">
    <div asp-validation-summary="All"></div>
    <div class="form-group">
        <label class="col-form-label " asp-for="Code"> </label>
        <input asp-for="Code" class="form-control" />
    </div>

    <div class="form-group">
        <label class="col-form-label " asp-for="Name"> </label>
        <input asp-for="Name" class="form-control" />
    </div>
    <div class="form-group">
        <label class="col-form-label " asp-for="DateOfCreation"> </label>
        <input asp-for="DateOfCreation" class="form-control" />
    </div>

    <input type="submit" value="Delete" class="btn btn-danger" />
    <a asp-action="Index" class="btn btn-light">Back To List</a>
</form>
