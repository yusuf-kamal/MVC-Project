@model ApplicationUser
@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>
<br />
<br />
<div class="row">
    <div class="col-12">
        <div class="form-group">
            <label asp-for="UserName" class="col-form-label ">  </label>
            <input asp-for="UserName" class="form-control" readonly />
        </div>

        <div class="form-group">
            <label class="col-form-label " asp-for="Email"> </label>
            <input asp-for="Email" class="form-control" readonly />
        </div>
        <div class="form-group">
            <label class="col-form-label " asp-for="PhoneNumber"> </label>
            <input asp-for="PhoneNumber" class="form-control" readonly />
        </div>
        <br />
        <a asp-action="Update" asp-route-id="@Model.Id"  class="btn btn-secondary">Edit</a>
        <a asp-action="Index" class="btn btn-light">Back To List</a>
    </div>
</div>

